{
	"name": "library-typescript",
	"version": "1.0.0-beta.0",
	"description": "A library written in typescript.",
	"main": "dist/node/index.js",
	"browser": "dist/es/index.js",
	"scripts": {
		"compile:node": "tsc -p tsconfig-node.json",
		"compile:es": "tsc",
		"compile": "rimraf dist && concurrently npm:compile:node npm:compile:es",
		"test": "npm run compile:node && nyc -- ava -v",
		"test:watch": "npm run compile:node && concurrently -n tsc,ava \"tsc -w -p tsconfig-node.json\" \"ava -w -v\"",
		"prepack": "npm run test && npm run compile"
	},
	"dependencies": {},
	"devDependencies": {
		"ava": "^1.4.1",
		"concurrently": "^4.1.0",
		"nyc": "^13.3.0",
		"rimraf": "^2.6.3",
		"typescript": "^3.4.1"
	},
	"author": "Max J. Polster",
	"license": "MIT"
}
