{
	"name": "library-typescript-node",
	"version": "1.0.0-beta.0",
	"description": "A node specific library written in typescript.",
	"main": "dist/index.js",
	"scripts": {
		"compile": "rimraf dist && tsc",
		"compile:watch": "rimraf dist && tsc -w",
		"test": "npm run compile && nyc -- ava -v",
		"test:watch": "npm run compile && concurrently -n tsc,ava \"tsc -w\" \"ava -w -v\"",
		"prepack": "npm run test && npm run compile"
	},
	"dependencies": {},
	"devDependencies": {
		"ava": "^1.3.1",
		"concurrently": "^4.1.0",
		"nyc": "^13.3.0",
		"rimraf": "^2.6.3",
		"typescript": "^3.3.3333"
	},
	"author": "Max J. Polster",
	"license": "MIT"
}
